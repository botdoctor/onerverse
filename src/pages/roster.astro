---
import Layout from '../layouts/Layout.astro';
import FloatingNavbar from '../components/FloatingNavbar.astro';
---

<Layout title="Roster - Onerverse">
  <div class="relative min-h-screen overflow-hidden bg-black text-white">
    <!-- Floating navbar with liquid glass effect -->
    <FloatingNavbar />
    
    <!-- Hero section -->
    <div class="relative z-10 pt-32 pb-20 px-6">
      <div class="max-w-7xl mx-auto">
        <div class="text-center mb-16">
          <span class="px-3 py-1 text-xs font-medium text-white bg-white bg-opacity-10 backdrop-blur-sm rounded-full mb-8 border border-white border-opacity-20 inline-block">Collaborators</span>
          <h1 class="text-5xl md:text-6xl font-bold bg-gradient-to-r from-purple-400 via-pink-500 to-blue-500 bg-clip-text text-transparent mb-6">
            Our Roster
          </h1>
          <p class="text-lg md:text-xl text-gray-300 max-w-3xl mx-auto">
            We collaborate with exceptional creators and influencers who share our vision for digital innovation
          </p>
        </div>
        
        <!-- Talent Grid -->
        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
          <!-- Selina Groetzner -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-purple-500 to-pink-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                SG
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Selina Groetzner</h3>
                <a href="https://instagram.com/groetzner.selina" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@groetzner.selina</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/groetzner.selina" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>
          
          <!-- Eva Meganosova -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-blue-500 to-cyan-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                EM
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Eva Meganosova</h3>
                <a href="https://instagram.com/evameganosova" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@evameganosova</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/evameganosova" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>
          
          <!-- Eileen Sziv -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-green-500 to-teal-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                ES
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Eileen Sziv</h3>
                <a href="https://instagram.com/eileensziv" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@eileensziv</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/eileensziv" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>
          
          <!-- Renee -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-orange-500 to-red-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                RR
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Renee</h3>
                <a href="https://instagram.com/itsreneesrealm" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@itsreneesrealm</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/itsreneesrealm" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>
          
          <!-- Foopah -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-indigo-500 to-purple-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                FP
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Foopah</h3>
                <a href="https://instagram.com/foopahh__" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@foopahh__</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/foopahh__" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>
          
          <!-- Alyssa Griffith -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-pink-500 to-rose-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                AG
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Alyssa Griffith</h3>
                <a href="https://instagram.com/alyssagriffith" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@alyssagriffith</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/alyssagriffith" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>

          <!-- Definitely First -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-yellow-500 to-orange-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                DF
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Definitely First</h3>
                <a href="https://instagram.com/definitelyfirst" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@definitelyfirst</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/definitelyfirst" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>

          <!-- Echo Ally -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-cyan-500 to-blue-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                EA
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Echo Ally</h3>
                <a href="https://instagram.com/echo.ally" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@echo.ally</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/echo.ally" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>

          <!-- Zeph Oner -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-red-500 to-pink-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                ZO
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Zeph Oner</h3>
                <a href="https://instagram.com/zeph.oner" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@zeph.oner</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/zeph.oner" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>

          <!-- Brandan Josh -->
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-6 hover:bg-opacity-10 transition-all duration-300">
            <div class="flex items-center mb-4">
              <div class="w-16 h-16 bg-gradient-to-br from-teal-500 to-green-500 rounded-full flex items-center justify-center text-white font-bold text-xl">
                BJ
              </div>
              <div class="ml-4">
                <h3 class="text-white font-semibold text-lg">Brandan Josh</h3>
                <a href="https://instagram.com/brandanjosh" target="_blank" class="text-gray-400 text-sm hover:text-white transition-colors">@brandanjosh</a>
              </div>
            </div>
            <div class="flex space-x-3">
              <a href="https://instagram.com/brandanjosh" target="_blank" class="text-xs px-3 py-1 bg-white bg-opacity-10 rounded-full hover:bg-opacity-20 transition-colors">Instagram</a>
            </div>
          </div>
        </div>
        
        <!-- CTA Section -->
        <div class="mt-20 text-center">
          <div class="bg-white bg-opacity-5 backdrop-blur-md rounded-2xl border border-white border-opacity-10 p-12 max-w-3xl mx-auto">
            <h2 class="text-3xl font-bold mb-4">Join Our Network</h2>
            <p class="text-gray-300 mb-8">Are you a creator interested in collaborating with Onerverse? We're always looking for talented individuals to join our growing network.</p>
            <a href="#" class="inline-block px-8 py-3 bg-white text-black font-medium rounded-full hover:bg-gray-200 transition-all duration-300 shadow-lg hover:shadow-xl">
              Apply to Collaborate
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</Layout>

<style>
  html, body {
    margin: 0;
    padding: 0;
    overflow-x: hidden;
  }
</style>